def max(a, b):
    if a > b:
        return a
    return b

def coin_row(coins):
    n = len(coins)

    if n == 1:
        return coins[0]
    
    if n == 2:
        return max(coins[0], coins[1])

    F = [0] * (n + 1)
    F[0] = 0
    F[1] = coins[0]

    for i in range(2, n + 1):
        F[i] = max(coins[i - 1] + F[i - 2], F[i - 1])

    return F[n]